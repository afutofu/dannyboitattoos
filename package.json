  "bugs": {
    "url": "https://github.com/afutofu/dannyboitattoos/issues"
  },
  "homepage": "https://github.com/afutofu/dannyboitattoos#readme",
  "dependencies": {
    "axios": "^0.27.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.19"
  }
}
